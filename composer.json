{
    "name": "patrickrobrecht/flexible-event-and-booking-manager",
    "type": "project",
    "description": "Manage events, their booking forms and bookings via a web-based platform.",
    "license": "proprietary",
    "require": {
        "php": "^8.3",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "barryvdh/laravel-dompdf": "^3.1",
        "guzzlehttp/guzzle": "^7.10",
        "intervention/validation": "^4.6",
        "laravel/framework": "^12.43",
        "laravel/sanctum": "^4.2",
        "laravel/tinker": "^2.10",
        "livewire/livewire": "^3.7",
        "phpoffice/phpspreadsheet": "^5.3",
        "portavice/bladestrap": "^1.5",
        "spatie/laravel-query-builder": "^6.3",
        "spatie/laravel-sluggable": "^3.7",
        "stechstudio/laravel-zipstream": "^5.7"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16",
        "fakerphp/faker": "^1.24",
        "kkomelin/laravel-translatable-string-exporter": "^1.25",
        "larastan/larastan": "^3.8",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.8",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpunit/phpunit": "^12.5",
        "portavice/laravel-pint-config": "^3.0",
        "spatie/laravel-ignition": "^2.9"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "cs": [
            "@csPHP",
            "npm run cs"
        ],
        "csPHP": [
            "pint --config vendor/portavice/laravel-pint-config/pint.json --test"
        ],
        "csfix": [
            "@csfixPHP",
            "npm run csfix"
        ],
        "csfixPHP": [
            "pint --config vendor/portavice/laravel-pint-config/pint.json"
        ],
        "production": [
            "@composer install --optimize-autoloader --no-dev"
        ],
        "stan": [
            "phpstan analyse --memory-limit=1G"
        ],
        "test": [
            "phpunit"
        ],
        "translate": [
            "@php artisan translatable:export de"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "audit": {
            "abandoned": "fail"
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
