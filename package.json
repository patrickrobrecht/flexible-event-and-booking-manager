{
    "private": true,
    "scripts": {
        "cs": "npm run csCSS && npm run csJS",
        "csCSS": "stylelint resources/sass/*.scss",
        "csJS": "eslint resources/js/*.js eslint.config.cjs vite.config.mjs",
        "csfix": "npm run csfixCSS && npm run csfixJS",
        "csfixCSS": "stylelint resources/sass/*.scss --fix",
        "csfixJS": "eslint resources/js/*.js eslint.config.cjs vite.config.mjs --fix",
        "dev": "vite",
        "build": "vite build"
    },
    "devDependencies": {
        "@eslint/js": "^9.34.0",
        "@fortawesome/fontawesome-free": "^7.0.0",
        "alpinejs": "^3.14.9",
        "axios": "^1.11.0",
        "bootstrap": "^5.3.8",
        "eslint": "^9.34.0",
        "laravel-vite-plugin": "^2.0.1",
        "rapidoc": "^9.3.8",
        "sass": "^1.91.0",
        "sass-loader": "^16.0.5",
        "stylelint": "^16.23.1",
        "stylelint-config-recommended-scss": "^16.0.0",
        "vite": "^7.1.3",
        "vite-plugin-static-copy": "^3.1.2"
    },
    "stylelint": {
        "extends": "stylelint-config-recommended-scss",
        "plugins": [
            "stylelint-scss"
        ]
    }
}
