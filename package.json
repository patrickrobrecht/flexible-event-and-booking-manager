{
    "private": true,
    "scripts": {
        "cs": "npm run csCSS && npm run csJS",
        "csCSS": "stylelint resources/sass/*.scss",
        "csJS": "eslint resources/js/*.js eslint.config.cjs vite.config.mjs",
        "csfix": "npm run csfixCSS && npm run csfixJS",
        "csfixCSS": "stylelint resources/sass/*.scss --fix",
        "csfixJS": "eslint resources/js/*.js eslint.config.cjs vite.config.mjs --fix",
        "dev": "vite",
        "build": "vite build"
    },
    "devDependencies": {
        "@eslint/js": "^10.0.1",
        "@fortawesome/fontawesome-free": "^7.2.0",
        "alpinejs": "^3.15.8",
        "axios": "1.13.5",
        "bootstrap": "^5.3.8",
        "eslint": "^10.0.1",
        "laravel-vite-plugin": "^2.1.0",
        "rapidoc": "^9.3.8",
        "sass": "^1.97.3",
        "stylelint": "^17.3.0",
        "stylelint-config-recommended-scss": "^17.0.0",
        "vite": "^7.3.1",
        "vite-plugin-static-copy": "^3.2.0"
    },
    "stylelint": {
        "extends": "stylelint-config-recommended-scss",
        "plugins": [
            "stylelint-scss"
        ]
    }
}
